/ items = leibie2
/ valign = top
/ halign = right
/ position = (95%, 5%)
/ txcolor = (0, 255, 0)
/ fontstyle = ("宋体", 5%)
</text>

<text leibieci1leftmixed>
/ items = leibie1
/ valign = top
/ halign = left
/ position = (5%, 19%)
/ txcolor = (0, 255, 0)
